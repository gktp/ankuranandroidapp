<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.paypal.ankuran">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.ankuran.AppMain"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ankuran.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ankuran.ui.WorkersProfileListActivity" />
        <activity android:name="com.ankuran.ui.CalculateWageActivity" />
        <activity android:name="com.ankuran.ui.SocialReportActivity" />
        <activity android:name="com.ankuran.ui.InventoryDetailsActivity" />
        <activity android:name="com.ankuran.ui.AddNewWorker"></activity>
    </application>

</manifest>